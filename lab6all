import math
a = float(input("Ввести нижню границю:"))
b = float(input("Ввести верхню границю:"))
h = float(input("Ввести значення кроку:"))
x = 0.0
spisok=[]
spisok1=[]
while a <= x <=b:
    y = (math.sin(x)*math.cos(2*x)/math.pow(3,2*x-3))
    spisok.append(y)
    spisok1.append(y)
    print("x=%.1f y=%.5f"%(x,y))
    x = x + h
print(spisok)
